<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zguming.orangeschool">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.OrderDetail" />
        <activity android:name=".activity.MoreOrderState" />
        <activity android:name=".activity.GoodsDetail" />
        <activity android:name=".activity.CheckBill" />
        <activity android:name=".activity.MyAddress"></activity>
        <activity android:name=".activity.CustomerService" />
        <activity android:name=".activity.Setting" />
        <activity android:name=".activity.MyInformation" />
        <activity android:name=".activity.ChangePassword" />
        <activity android:name=".activity.ChangePhoneNum" />
        <activity android:name=".activity.SystemNotify" />
        <activity android:name=".activity.NotifyContent" />
    </application>

</manifest>